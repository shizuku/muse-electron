{
  "name": "muse-electron",
  "productName": "Muse",
  "version": "0.1.0",
  "description": "A react electron project to show and edit music score.",
  "homepage": "https://shizuku.github.io/muse-electron/",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "predeploy": "yarn docs:build",
    "deploy": "gh-pages -d docs/.vuepress/dist"
  },
  "keywords": [
    "electron",
    "electron-forge",
    "webpack",
    "react",
    "typescript",
    "antd",
    "mobx",
    "music",
    "muse"
  ],
  "author": {
    "name": "shizuku",
    "email": "2112165916@qq.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "public/code.ico",
        "name": "Muse",
        "executableName": "Muse",
        "win32metadata": "muse/muse"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        },
        {
          "name": "@electron-forge/maker-squirrel",
          "platforms": [
            "win32"
          ],
          "config": {
            "name": "Muse",
            "exe": "Muse",
            "title": "Muse",
            "noMsi": false
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "platforms": [
            "linux"
          ],
          "config": {
            "options": {
              "bin": "Muse",
              "name": "Muse",
              "productName": "Muse",
              "genericName": "Muse"
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "platforms": [
            "linux"
          ],
          "config": {
            "options": {
              "bin": "Muse",
              "name": "Muse",
              "productName": "Muse",
              "genericName": "Muse"
            }
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "platforms": [
            "darwin"
          ],
          "config": {
            "format": "ULFO"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/pages/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.53",
    "@electron-forge/maker-deb": "6.0.0-beta.53",
    "@electron-forge/maker-dmg": "^6.0.0-beta.53",
    "@electron-forge/maker-rpm": "6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "6.0.0-beta.53",
    "@electron-forge/maker-zip": "6.0.0-beta.53",
    "@electron-forge/plugin-webpack": "6.0.0-beta.53",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "css-loader": "^4.2.1",
    "electron": "10.1.3",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "gh-pages": "^3.1.0",
    "node-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.2",
    "vuepress": "^1.7.1"
  },
  "dependencies": {
    "@types/classnames": "^2.2.10",
    "@types/electron": "^1.6.10",
    "@types/electron-store": "^3.2.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "antd": "^4.6.6",
    "classnames": "^2.2.6",
    "electron-is": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.1",
    "hotkeys-js": "^3.8.1",
    "html2canvas": "^1.0.0-rc.7",
    "i18next": "^19.8.2",
    "i18next-browser-languagedetector": "^6.0.1",
    "mobx": "^5.15.6",
    "mobx-react": "^6.3.0",
    "react": "^16.13.1",
    "react-dnd": "^11.1.3",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.3"
  }
}
